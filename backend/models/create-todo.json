{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "TableName": {
      "type":"string"
    },
    "Item": {
      "type": "object",
      "properties": {
        "name": {
          "type":"string",
          "minLength": 3
        },
        "userId": {
          "type":"string"
        },
        "todoId": {
          "type":"string"
        },
        "createdAt": {
          "type":"string"
        }
      },
      "required": [
        "name", "userId", "todoId", "createdAt"
      ]
    }
  },
  "required": [
    "Item", "TableName"
  ],
  "additionalProperties": false
}



